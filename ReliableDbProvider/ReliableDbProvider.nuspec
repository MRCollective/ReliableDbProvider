<?xml version='1.0' encoding='UTF-8'?> 
<package>
  <metadata>
    <id>
       ReliableDbProvider
    </id>
    <version>
       1.0.0
    </version>
    <authors>
       Robert Moore
    </authors>
    <description>
      Provides a Db Provider Factory that uses the Microsoft Transient Fault Handling library to allow for reliable SQL Azure connections when using Entity Framework 4, Entity Framework 5 or Linq 2 SQL.
      This package has the Microsoft Transient Fault Handling library v5 IL-merged into it; for a version that doesn't see the ReliableDbProvider.Standalone package.
    </description>
    <projectUrl>
      https://github.com/robdmoore/ReliableDbProvider
    </projectUrl>
    <licenseUrl>
      https://github.com/robdmoore/ReliableDbProvider/blob/master/LICENSE
    </licenseUrl>
    <iconUrl>
      https://raw.github.com/robdmoore/ReliableDbProvider/master/logo.png
    </iconUrl>
    <tags>
       entityframework, ef, linq, linqtosql, linq2sql, l2s, azure, sql, sqlazure, transient
    </tags>
    <language>
       en-US 
    </language>
  </metadata>
  <files>
     <file src="bin\Release\Combined\ReliableDbProvider.dll" target="lib\NET40" />
     <file src="bin\Release\Combined\ReliableDbProvider.pdb" target="lib\NET40" />
     <file src="bin\Release\Combined\ReliableDbProvider.XML" target="lib\NET40" />
  </files>
</package>